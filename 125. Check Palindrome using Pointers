
#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    printf("Enter string: ");
    scanf("%s", str);

    char *p = str;
    char *q = str + strlen(str) - 1;

    int flag = 1;

    while(p < q) {
        if(*p != *q) {
            flag = 0;
            break;
        }
        p++;
        q--;
    }

    if(flag)
        printf("Palindrome");
    else
        printf("Not Palindrome");

    return 0;
}
