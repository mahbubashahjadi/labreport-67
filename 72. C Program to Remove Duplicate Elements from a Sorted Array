#include <stdio.h>

int main() {
    int n; scanf("%d",&n);
    int a[n]; for(int i=0;i<n;i++) scanf("%d",&a[i]);
    if(n==0){ printf("0\n"); return 0; }
    int idx = 1;
    for(int i=1;i<n;i++) if(a[i] != a[i-1]) a[idx++] = a[i];
    for(int i=0;i<idx;i++) printf("%d ", a[i]);
    printf("\nNew length = %d\n", idx);
    return 0;
}
