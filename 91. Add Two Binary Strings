
#include <stdio.h>
#include <string.h>

int main() {
    char a[100], b[100], result[200];
    int i, j, k = 0, carry = 0;

    gets(a);
    gets(b);

    i = strlen(a) - 1;
    j = strlen(b) - 1;

    while(i >= 0 || j >= 0 || carry) {
        int sum = carry;
        if(i >= 0) sum += a[i--] - '0';
        if(j >= 0) sum += b[j--] - '0';

        result[k++] = (sum % 2) + '0';
        carry = sum / 2;
    }

    result[k] = '\0';

    strrev(result);
    printf("Sum = %s", result);
    return 0;
}
