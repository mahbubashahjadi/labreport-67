
#include <stdio.h>

int main() {
    char a[100], b[100];
    gets(a);
    gets(b);

    int i=0;

    while(a[i] && b[i]) {
        if(a[i] < b[i]) { printf("a < b"); return 0; }
        if(a[i] > b[i]) { printf("a > b"); return 0; }
        i++;
    }

    if(a[i] == '\0' && b[i] == '\0') printf("a == b");
    else if(a[i] == '\0') printf("a < b");
    else printf("a > b");

    return 0;
}
