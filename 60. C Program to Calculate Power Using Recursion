#include <stdio.h>

long long power(long long base, int exp) {
    if(exp == 0) return 1;
    if(exp % 2 == 0) {
        long long half = power(base, exp/2);
        return half * half;
    } else {
        return base * power(base, exp - 1);
    }
}

int main() {
    long long base; int exp;
    scanf("%lld %d", &base, &exp);
    if(exp < 0) { printf("Negative exponent not supported\n"); return 0; }
    printf("%lld\n", power(base, exp));
    return 0;
}
