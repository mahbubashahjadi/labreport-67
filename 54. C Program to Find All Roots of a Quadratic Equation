#include <stdio.h>
#include <math.h>

int main() {
    double a, b, c;
    scanf("%lf %lf %lf", &a, &b, &c);

    if(a == 0) {
        if(b == 0) printf("No valid equation\n");
        else printf("Linear root = %.6lf\n", -c / b);
        return 0;
    }

    double d = b*b - 4*a*c;
    if(d > 0) {
        double r1 = (-b + sqrt(d)) / (2*a);
        double r2 = (-b - sqrt(d)) / (2*a);
        printf("Two real roots: %.6lf and %.6lf\n", r1, r2);
    } else if(d == 0) {
        double r = -b / (2*a);
        printf("One real root: %.6lf\n", r);
    } else {
        double real = -b / (2*a);
        double imag = sqrt(-d) / (2*a);
        printf("Complex roots: %.6lf + %.6lfi and %.6lf - %.6lfi\n", real, imag, real, imag);
    }
    return 0;
}
