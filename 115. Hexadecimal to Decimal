
#include <stdio.h>
#include <math.h>
#include <string.h>

int main() {
    char hex[20];
    gets(hex);
    int len = strlen(hex), dec = 0, base = 1;

    for(int i=len-1; i>=0; i--) {
        if(hex[i] >= '0' && hex[i] <= '9')
            dec += (hex[i] - '0') * base;
        else
            dec += (hex[i] - 'A' + 10) * base;
        base *= 16;
    }
    printf("%d", dec);
    return 0;
}
