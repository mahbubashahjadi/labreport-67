
#include <stdio.h>
#include <stdlib.h>

int cmpdesc(const void *p, const void *q) {
    return (*(int*)q - *(int*)p);
}

int main() {
    int n; scanf("%d",&n);
    int a[n]; for(int i=0;i<n;i++) scanf("%d",&a[i]);
    qsort(a, n, sizeof(int), cmpdesc);
    for(int i=0;i<n;i++) printf("%d ", a[i]); printf("\n");
    return 0;
}
