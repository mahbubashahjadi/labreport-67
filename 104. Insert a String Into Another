
#include <stdio.h>
#include <string.h>

int main() {
    char s1[200], s2[100], result[300];
    int pos, i=0, j=0, k=0;

    gets(s1);
    gets(s2);
    scanf("%d", &pos);

    while(i < pos) result[k++] = s1[i++];

    while(s2[j]) result[k++] = s2[j++];

    while(s1[i]) result[k++] = s1[i++];

    result[k] = '\0';

    printf("Result: %s", result);
    return 0;
}
