
#include <stdio.h>

struct Student {
    char name[50];
    int roll;
};

int main() {
    struct Student s1 = {"Amina", 10}, s2;

    FILE *f = fopen("stud.dat", "wb");
    fwrite(&s1, sizeof(s1), 1, f);
    fclose(f);

    f = fopen("stud.dat", "rb");
    fread(&s2, sizeof(s2), 1, f);
    fclose(f);

    printf("%s %d", s2.name, s2.roll);
    return 0;
}
