#include <stdio.h>

int main(){
    int a[10][10], n, i, j;
    int mainD = 0, secD = 0;

    printf("Enter size of square matrix: ");
    scanf("%d",&n);

    printf("Enter elements:\n");
    for(i=0;i<n;i++)
        for(j=0;j<n;j++)
            scanf("%d",&a[i][j]);

    for(i=0;i<n;i++){
        mainD += a[i][i];
        secD  += a[i][n-1-i];
    }

    printf("\nMain diagonal sum = %d\n", mainD);
    printf("Secondary diagonal sum = %d\n", secD);
}
