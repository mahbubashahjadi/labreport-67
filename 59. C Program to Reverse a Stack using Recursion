#define MAX 100

int stack[MAX], top = -1;

void push(int x) {
    if(top < MAX - 1) stack[++top] = x;
}

int pop() {
    if(top >= 0) return stack[top--];
    return -1; // underflow sentinel
}

int isEmpty() {
    return top == -1;
}

void insertAtBottom(int x) {
    if(isEmpty()) {
        push(x);
        return;
    }
    int t = pop();
    insertAtBottom(x);
    push(t);
}

void reverseStack() {
    if(isEmpty()) return;
    int t = pop();
    reverseStack();
    insertAtBottom(t);
}

int main() {
    int n, val;
    scanf("%d", &n); // number of elements
    for(int i = 0; i < n; i++) {
        scanf("%d", &val);
        push(val);
    }

    reverseStack();

    // print stack elements from top to bottom (which is reversed original order)
    while(!isEmpty()) {
        printf("%d ", pop());
    }
    return 0;
}
