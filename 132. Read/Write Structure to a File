#include <stdio.h>

struct Student {
    char name[50];
    int age;
};

int main() {
    struct Student s1 = {"Rahim", 20}, s2;

    FILE *f = fopen("student.dat", "wb");
    fwrite(&s1, sizeof(s1), 1, f);
    fclose(f);

    f = fopen("student.dat", "rb");
    fread(&s2, sizeof(s2), 1, f);
    fclose(f);

    printf("%s %d", s2.name, s2.age);

    return 0;
}
